{
    "middleware": {
        "pages": { },
        "notFound": { },
        "debugInfo": { },
        "exceptionReporter": { },
        "staticFiles": {
            "assets": {
                "staticFilesRootUrl": "/assets",
                "documentationRootUrl": null,
                "rootDirectory": "~\\assets",
                "enabled": "true",
                "includeSubFolders": "true",
                "FileExtensions": [
                    { "extension": ".css", "mimeType": "text/css" },
                    { "extension": ".js", "mimeType": "application/javascript" },
                    { "extension": ".jpg", "mimeType": "image/jpeg" },
                    { "extension": ".jpeg", "mimeType": "image/jpeg" },
                    { "extension": ".png", "mimeType": "image/png" },
                    { "extension": ".eps", "mimeType": "application/postscript" },
                    { "extension": ".svg", "mimeType": "image/svg+xml" }
                ],
                "maximumFileSizeToCache": 10000,
                "totalCacheSize": 1000000,
                "maximumCacheTime": "02:00:00",
                "requiredPermission": ""
            }
        },
        "less": {
            "rootUrl": "/assets/styles",
            "rootDirectory": "~\\assets\\styles",
            "enabled": true,
            "analyticsEnabled": false,
            "minify": false,
            "traceLog": false
        }
    },
    "owinFramework": {
        "pages": {
            "framework": {
                "assetRootPath": "/assets",
                "assetVersion": "1",
                "defaultLanguage": "en-US",
                "debugLogging": true
            },
            "html": {
                "includeComments": false,
                "indented": true,
                "htmlFormat": "Html"
            }
        }
    }
}