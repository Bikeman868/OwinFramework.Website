{
  "middleware": {
    "pages": {},
    "notFound": {},
    "debugInfo": {},
    "exceptionReporter": {},
    "staticFiles": {
      "assets": {
        "staticFilesRootUrl": "/assets",
        "documentationRootUrl": null,
        "rootDirectory": "~\\assets",
        "enabled": "true",
        "includeSubFolders": "true",
        "FileExtensions": [
          {
            "extension": ".css",
            "mimeType": "text/css"
          },
          {
            "extension": ".js",
            "mimeType": "application/javascript"
          },
          {
            "extension": ".jpg",
            "mimeType": "image/jpeg"
          },
          {
            "extension": ".jpeg",
            "mimeType": "image/jpeg"
          },
          {
            "extension": ".png",
            "mimeType": "image/png"
          },
          {
            "extension": ".eps",
            "mimeType": "application/postscript"
          },
          {
            "extension": ".svg",
            "mimeType": "image/svg+xml"
          }
        ],
        "maximumFileSizeToCache": 0,
        "totalCacheSize": 0,
        "maximumCacheTime": "0",
        "requiredPermission": ""
      }
    },
    "less": {
      "rootUrl": "/assets/styles",
      "rootDirectory": "~\\assets\\styles",
      "enabled": true,
      "analyticsEnabled": false,
      "minify": true,
      "traceLog": false
    },
    "outputCache": {
      "rules": [
        {
          "category": "",
          "priority": "",
          "cacheCategory": "OutputCache",
          "serverCacheTime": "0",
          "browserCacheTime": "0"
        }
      ]
    },
    "versioning": {
      "version": 1,
      "mimeTypes": [ "text/html", "text/css" ],
      "fileExtensions": [".css", ".js", ".ico", ".svg", ".png"],
      "browserCacheTime": "0",
      "exactVersion": false,
      "analyticsEnabled": true
    }
  },
  "owinFramework": {
    "pages": {
      "framework": {
        "assetRootPath": "/assets",
        "assetVersion": "1",
        "defaultLanguage": "en-US",
        "debugLogging": true,
        "includeComments": true,
        "indented": false,
        "htmlFormat": "Html",
        "minifyCss": false,
        "minifyJavascript": false
      }
    }
  }
}